def print_grid():
    """
    Print the grid of boxes.
    """
    print('┌─┬─┬─┐\n'
          '│{}│{}│{}│\n'
          '├─┼─┼─┤\n'
          '│{}│{}│{}│\n'
          '├─┼─┼─┤\n'
          '│{}│{}│{}│\n'
          '└─┴─┴─┘\n'
          .format(*[box_display(boxes[i][j])
                    for i in range(3)
                    for j in range(3)]))